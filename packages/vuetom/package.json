{
  "name": "vitepress-theme-vuetom",
  "version": "2.0.1",
  "description": "Vitepress Theme Vuetom",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "types/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": {
          "default": "./dist/index.js"
        },
        "require": {
          "default": "./dist/index.js"
        }
      },
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "pub": "npm publish",
    "clean": "rimraf ./dist",
    "build": "pnpm clean && pnpm build:vt && pnpm build:scss && pnpm build:css",
    "build:vt": "tsc -p . && node ../../scripts/build-vt",
    "build:scss": "gulp --require sucrase/register/ts -f gulpfile.ts",
    "build:css": "npx tailwindcss -i ./styles/tailwind/s.css -o ./styles/tailwind/d.css",
    "post:css": "npx tailwindcss -i ./styles/tailwind/s.css -o ./styles/tailwind/d.css --watch"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "types",
    "vuetom"
  ],
  "keywords": [
    "vitepress",
    "theme"
  ],
  "author": "lauset",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lauset/vitepress-theme-vuetom.git"
  },
  "bugs": {
    "url": "https://github.com/lauset/vitepress-theme-vuetom/issues"
  },
  "homepage": "https://github.com/lauset/vitepress-theme-vuetom#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "vitepress": "1.0.0-alpha.4"
  },
  "dependencies": {
    "@docsearch/css": "^3.0.0",
    "@docsearch/js": "^3.0.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "shiki": "^0.10.1"
  }
}
